env:
  browser: true
  es2021: true
extends:
  - 'eslint:recommended'
  - 'plugin:@typescript-eslint/recommended'
  - 'prettier'
parser: '@typescript-eslint/parser'
parserOptions:
  ecmaVersion: latest
  sourceType: module
  tsconfigRootDir: .
  project: ['./tsconfig.json']
plugins:
  - '@typescript-eslint'
ignorePatterns:
  - dist/index.js
rules: {}
